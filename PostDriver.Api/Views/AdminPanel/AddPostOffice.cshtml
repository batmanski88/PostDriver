@model AddPostOfficeViewModel

<section>
    <div class="row">
        <form method="post" class="form-horizontal" asp-controller="AdminPanel" asp-action="AddCompany">
                <div class="form-group">
                    <label asp-for="Name" class="col-md-2"></label>
                    <div class="col-md-4">
                        <input asp-for="Name" class="form-control" />
                        <span asp-validation-for="Name"></span>
                    </div>
                </div>
                 <div class="form-group">
                    <label asp-for="Adress" class="col-md-2"></label>
                    <div class="col-md-4">
                        <input asp-for="Adress" class="form-control" />
                        <span asp-validation-for="Adress"></span>
                    </div>
                </div>
                 <div class="form-group">
                    <label asp-for="Latitude" class="col-md-2"></label>
                    <div class="col-md-4">
                        <input asp-for="Latitude" class="form-control" />
                        <span asp-validation-for="Latitude"></span>
                    </div>
                </div>
                 <div class="form-group">
                    <label asp-for="Longitude" class="col-md-2"></label>
                    <div class="col-md-4">
                        <input asp-for="Longitude" class="form-control" />
                        <span asp-validation-for="Longitude"></span>
                    </div>
                </div>
        </form>
    </div>
</section>

